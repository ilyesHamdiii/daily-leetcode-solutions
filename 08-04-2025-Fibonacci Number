#https://leetcode.com/problems/fibonacci-number/

# Difficulty:Easy

# Time: O(n)

# Space: O(n)
class Solution(object):
    def fib(self, n):
        if n==0:
            return 0
        if n==1:
            return 1
        curr=1
        prev=0
        for i in range(2,n+1):
            prev,curr=curr,prev+curr
        return curr


        """
        :type n: int
        :rtype: int
        """
        