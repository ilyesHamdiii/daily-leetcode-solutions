#https://leetcode.com/problems/find-pivot-index/description/

# Difficulty:Eas

# Time: O(n)

# Space: O(n)
class Solution:
    def pivotIndex(self, nums: List[int]) -> int:
        i=0
        while sum(nums[:i])!=sum(nums[i+1:]):
            i+=1
            if i>=len(nums):
                return -1
        return i 
        