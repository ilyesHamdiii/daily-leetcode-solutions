# Problem: https://leetcode.com/problems/removing-stars-from-a-string/description/
# Difficulty: Easy


# Time: O(n)
# Space: O(n)
class Solution(object):
    def removeStars(self, s):
        stack=[]
        for i in range (len(s)):
            if(s[i]!="*"):
                stack.append(s[i])
            else:
                stack.pop()
        s=''
        return ''.join(stack)
        """
        :type s: str
        :rtype: str
        """
        