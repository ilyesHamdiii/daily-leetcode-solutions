# Problem:https://leetcode.com/problems/reverse-linked-list/description/
# Difficulty: Easy


# Time: O(n)
# Space: O(n)

class Solution:
    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        node =None
        while head:
            temp=head.next
            head.next=node
            node=head
            head=temp

        return node        