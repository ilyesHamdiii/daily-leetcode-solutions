class Solution(object):
    def removeStars(self, s):
        stack=[]
        for i in range (len(s)):
            if(s[i]!="*"):
                stack.append(s[i])
            else:
                stack.pop()
        s=''
        return ''.join(stack)
        """
        :type s: str
        :rtype: str
        """
        