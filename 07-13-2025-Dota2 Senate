# Problem:https://leetcode.com/problems/dota2-senate/
# Difficulty: Medium


# Time: O(n*n)
# Space: O(n)









class Solution:
    def predictPartyVictory(self, senate):
        n=len(senate)
        R_idx=deque([i for i in range(n) if senate[i]=='R'])
        D_idx=deque([j for j in range(n) if senate[j]=='D'])
        while R_idx and D_idx:
            r=R_idx.popleft()
            d=D_idx.popleft()

            if r<d:
                R_idx.append(n+r)

            else:
                D_idx.append(n+d)


        return 'Radiant' if R_idx else 'Dire'            